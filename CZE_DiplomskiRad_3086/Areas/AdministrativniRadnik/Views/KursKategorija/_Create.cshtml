@model CZE.Web.Areas.AdministrativniRadnik.Models.KursVMs.KursKategorijaVM
@{
  Layout = null;
}
<!-- Breadcrumbs-->
<ol class="breadcrumb mb-0">
  <li class="breadcrumb-item">
    <a asp-area="AdministrativniRadnik" asp-controller="KursKategorija" asp-action="GetKursKategorijaTable"
       ajax-call="da" ajax-update="#kursKategorijaCardBody">Kategorija</a>
  </li>
  <li class="breadcrumb-item active">Create</li>
</ol>

<form asp-area="AdministrativniRadnik" asp-controller="KursKategorija" asp-action="Create" ajax-call="da" ajax-update="#kursKategorijaCardBody">
  <div class="form-group">
    <input asp-for="KursKategorijaId" class="form-control" type="hidden" />
  </div>
  <div class="form-group">
    <label asp-for="Naziv" class="col-form-label"></label>
    <input asp-for="Naziv" class="form-control"/>
    <span asp-validation-for="Naziv" class="text-danger"></span>
  </div>
  <button type="submit" class="btn btn-primary">Snimi</button>
</form>